{
  "name": "theinterned.net",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://theinterned.net",
  "dependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4",
    "styled-components": "^3.4.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages --dist build --branch master --repo https://github.com/theinterned/theinterned.github.io.git"
  },
  "eslintConfig": {
    "extends": ["react-app", "prettier", "prettier/react"]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^3.1.0"
  }
}
